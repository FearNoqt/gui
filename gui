local function showTab(tab)
	Main.Visible = false
	Tools.Visible = false
	Global.Visible = false
	Misc.Visible = false
	Keybinds.Visible = false
	tab.Visible = true
end

MainB.MouseButton1Click:Connect(function()
	showTab(Main)
end)

ToolsB.MouseButton1Click:Connect(function()
	showTab(Tools)
end)

GlobalB.MouseButton1Click:Connect(function()
	showTab(Global)
end)

MiscB.MouseButton1Click:Connect(function()
	showTab(Misc)
end)

KeybindsB.MouseButton1Click:Connect(function()
	showTab(Keybinds)
end)

local colorScript = Instance.new("LocalScript")
colorScript.Name = "ColorCycle"
colorScript.Parent = Line
colorScript.Source = [[
	while true do
		script.Parent.BackgroundColor3 = Color3.fromRGB(math.random(0,255), math.random(0,255), math.random(0,255))
		wait(0.5)
	end
]]
